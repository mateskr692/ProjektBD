<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Presentation.App.Resources">
    <!--Colors-->
    <Color x:Key="ColorPrimary1">#339933</Color>
    <Color x:Key="ColorPrimary2">#194d19</Color>
    
    <!--Background-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}"/>

</ResourceDictionary>